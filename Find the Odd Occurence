https://practice.geeksforgeeks.org/problems/find-the-odd-occurence/0





#include<bits/stdc++.h>
#define int long long int 
#define ALL_THE_BEST_ANSHU ios_base::sync_with_stdio(false);  cin.tie(NULL);  cout.tie(NULL);
using namespace std;

signed main()
 {
	ALL_THE_BEST_ANSHU ;
	
	
	int t ;
	cin >> t ;
	while ( t-- )
	{
	    int ans ;
	    int n , a ;
	    cin >> n ;
	    cin >> a ;
	    n-- ;
	    ans = a ;
	    while ( n-- )
	    {
	        cin >> a ;
	        ans = ans xor a ;
	    }
	    cout << ans << endl ;
	    
	}
	
	
}
