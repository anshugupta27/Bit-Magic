#include<bits/stdc++.h>
#define int long long int 
#define TRY_HARD ios_base::sync_with_stdio(false);  cin.tie(NULL);  cout.tie(NULL);
using namespace std;


signed main()
 {
	TRY_HARD ;
	int t ;
	cin >> t ;
	while ( t-- )
	{
	    int n ;
	    cin >> n ;
	    cout << __builtin_popcount ( n ) << endl ;
	}
	
}
